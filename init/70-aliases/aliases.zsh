# Zsh Aliases

alias sudo='sudo '
alias open=kde-open5
alias o=kde-open5

# Safety
alias cp='cp -i'
alias rm='rm -I'
alias mv='mv -i'

alias diff='diff --color=auto'
alias du='du -h'
alias hd='hexdump -C'
alias ls='ls --color=auto'
alias md='mkdir -p'
alias println='print -l'
alias rd='rmdir'
alias tp='trash-put'
alias whose='pacman --query --owns'
